	        knowledge.SetInputValue("inputAttr1", "C");
	        var results:Array = knowledge.EvaluateTableGet(tableName, true);
	        if (ArraySize(results) == 2 && results["outputAttr1"].length == 3 &&
	            results["outputAttr1"][0] == "2" &&
